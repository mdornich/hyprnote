  "components": {
    "schemas": {
      "BatchAlternatives": {
        "type": "object",
        "required": [
          "transcript",
          "confidence"
        ],
        "properties": {
          "confidence": {
            "type": "number",
            "format": "double"
          },
          "transcript": {
            "type": "string"
          },
          "words": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BatchWord"
            }
          }
        }
      },
      "BatchChannel": {
        "type": "object",
        "required": [
          "alternatives"
        ],
        "properties": {
          "alternatives": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BatchAlternatives"
            }
          }
        }
      },
      "BatchResponse": {
        "type": "object",
        "required": [
          "metadata",
          "results"
        ],
        "properties": {
          "metadata": {
            "type": "object"
          },
          "results": {
            "$ref": "#/components/schemas/BatchResults"
          }
        }
      },
      "BatchResults": {
        "type": "object",
        "required": [
          "channels"
        ],
        "properties": {
          "channels": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BatchChannel"
            }
          }
        }
      },
      "BatchWord": {
        "type": "object",
        "required": [
          "word",
          "start",
          "end",
          "confidence"
        ],
        "properties": {
          "confidence": {
            "type": "number",
            "format": "double"
          },
          "end": {
            "type": "number",
            "format": "double"
          },
          "punctuated_word": {
            "type": [
              "string",
              "null"
            ]
          },
          "speaker": {
            "type": [
              "integer",
              "null"
            ],
            "minimum": 0
          },
          "start": {
            "type": "number",
            "format": "double"
          },
          "word": {
            "type": "string"
          }
        }
      },
      "CanStartTrialReason": {
        "type": "string",
        "enum": [
          "eligible",
          "not_eligible",
          "error"
        ]
      },
      "CanStartTrialResponse": {
        "type": "object",

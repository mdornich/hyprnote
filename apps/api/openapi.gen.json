        "properties": {
          "appVersion": {
            "type": "string"
          },
          "arch": {
            "type": "string"
          },
          "buildHash": {
            "type": [
              "string",
              "null"
            ]
          },
          "locale": {
            "type": [
              "string",
              "null"
            ]
          },
          "osVersion": {
            "type": "string"
          },
          "platform": {
            "type": "string"
          }
        }
      },
      "EventAttendee": {

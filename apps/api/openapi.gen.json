    "/stt/listen": {
      "get": {
        "tags": [
          "stt"
        ],
        "operationId": "stt_listen_stream",
        "parameters": [
          {
            "name": "provider",
            "in": "query",
            "description": "STT provider. Use 'hyprnote' for automatic routing (default), or specify:\ndeepgram, soniox, assemblyai, gladia, elevenlabs, fireworks, openai, dashscope, mistral",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "model",
            "in": "query",
            "description": "Model to use for transcription (provider-specific, e.g. 'nova-3')",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "language",
            "in": "query",
            "description": "BCP-47 language hint (e.g. 'en', 'ko', 'ja'). Multiple values or comma-separated supported",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "keywords",
            "in": "query",
            "description": "Keyword boosting. Comma-separated or repeated query params",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sample_rate",
            "in": "query",
            "description": "Audio sample rate in Hz (default: 16000)",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 0
            }
          },
          {
            "name": "channels",
            "in": "query",
            "description": "Number of audio channels (default: 1)",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 0
            }
          },
          {
            "name": "encoding",
            "in": "query",
            "description": "Audio encoding: linear16, flac, mulaw, opus, ogg-opus, etc.",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "101": {
            "description": "WebSocket upgrade. Server sends StreamResponse JSON messages."
          },
          "400": {
            "description": "Bad request (invalid provider, missing params, etc.)"
          },
          "502": {
            "description": "Upstream provider connection failed"
          }
        }
      },
      "post": {
        "tags": [
          "stt"
        ],
        "operationId": "stt_listen_batch",
        "parameters": [
          {
            "name": "provider",
            "in": "query",
            "description": "STT provider. Use 'hyprnote' for automatic routing (default), or specify:\ndeepgram, soniox, assemblyai, gladia, elevenlabs, fireworks, openai, dashscope, mistral",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "model",
            "in": "query",
            "description": "Model to use for transcription (provider-specific, e.g. 'nova-3')",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "language",
            "in": "query",
            "description": "BCP-47 language hint (e.g. 'en', 'ko', 'ja'). Multiple values or comma-separated supported",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "keywords",
            "in": "query",
            "description": "Keyword boosting. Comma-separated or repeated query params",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "callback",
            "in": "query",
            "description": "When set, enables async callback mode. Body should be JSON with a `url` field instead of raw audio",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Raw audio bytes (sync mode) or JSON `{ \"url\": \"<file_id>\" }` (callback mode)",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ListenCallbackRequest"
              }
            },
            "application/octet-stream": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "integer",
                  "format": "binary",
                  "minimum": 0
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Transcription result (sync) or ListenCallbackResponse (callback)",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BatchResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (empty body, invalid provider, etc.)"
          },
          "401": {
            "description": "Unauthorized"
          },
          "502": {
            "description": "All upstream providers failed"
          }
        }
      }
    },

              "null"
            ],
            "items": {
              "$ref": "#/components/schemas/EventAttendee"
            }
          },
          "calendar_id": {
            "type": "string"
          },
          "connection_id": {
            "type": "string"
          },
          "description": {
            "type": [
              "string",
              "null"
            ]
          },
          "end": {
            "$ref": "#/components/schemas/EventDateTime"
          },
          "location": {
            "type": [
              "string",
              "null"
            ]
          },
          "start": {
            "$ref": "#/components/schemas/EventDateTime"
          },
          "summary": {
            "type": "string"
          }
        }
      },
      "CreateEventResponse": {
        "type": "object",
        "required": [

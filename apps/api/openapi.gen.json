        ],
        "properties": {
          "canStartTrial": {
            "type": "boolean",
            "example": true
          },
          "reason": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/CanStartTrialReason"
              }
            ]
          }
        }
      },
      "ConnectSessionResponse": {
        "type": "object",
        "required": [
          "token",
          "expires_at"
        ],
        "properties": {
          "expires_at": {
            "type": "string"
          },
          "token": {
            "type": "string"
          }
        }
      },
      "ConversationSummary": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "inboxId": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      },
      "CreateContactRequest": {
        "type": "object",
        "required": [
          "identifier"
        ],
        "properties": {
          "email": {
            "type": [
              "string",
              "null"
            ]
          },
          "identifier": {
            "type": "string"
          },
          "name": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      },
      "CreateContactResponse": {
        "type": "object",
        "required": [
          "sourceId",
